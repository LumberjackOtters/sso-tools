{
  "version": "2.0",
  "app_name": "ssoToolsAPI",
  "autogen_policy": false,
  "stages": {
    "dev": {
      "api_gateway_stage": "dev",
      "environment_variables": {
        "API_STAGE": "dev",
        "mongoURL": "mongodb://localhost",
